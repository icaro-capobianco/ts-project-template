{
	"name": "vite-react-chakra-template",
	"version": "1.0.0",
	"author": "√çcaro C. Capobianco",
	"license": "MIT",
	"source": "src/index.tsx",
	"main": "es5/index.js",
	"require": "es5/index.js",
	"import": "es6/index.js",
	"types" : "types/index.d.ts",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"start": "tsc && yarn dev",
		"es5": "tsc -p tsconfig.json --target es6 --outDir es6 --module es6",
		"es6": "tsc -p tsconfig.json --target es5 --outDir es5 --module commonjs",
		"tsc": "concurrently \"npm run es5\" \"npm run es6\"",
		"watch": "concurrently \"npm run es5 --watch\" \"npm run es6 --watch\"",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@chakra-ui/icons": "^1.0.15",
		"@chakra-ui/react": "^1.6.6",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"framer-motion": "^4.1.17",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.2.0"
	},
	"devDependencies": {
		"@storybook/cli": "^6.3.7",
		"@storybook/react": "^6.3.7",
		"@types/node": "^16.7.1",
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"concurrently": "^6.2.1",
		"sb": "^6.3.7",
		"typescript": "^4.3.5",
		"vite": "^2.5.0",
		"vite-aliases": "^0.8.6",
		"vite-preset-react": "^1.3.0"
	}
}
